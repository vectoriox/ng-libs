{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "viox-masonry": [
        "dist/viox-masonry"
      ],
      "viox-masonry/*": [
        "dist/viox-masonry/*"
      ],
      "viox-ui-direvetives": [
        "dist/viox-ui-direvetives"
      ],
      "viox-ui-direvetives/*": [
        "dist/viox-ui-direvetives/*"
      ],
      "viox-aos": [
        "dist/viox-aos"
      ],
      "viox-aos/*": [
        "dist/viox-aos/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}